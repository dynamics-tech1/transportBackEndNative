{
  "name": "transportwithnative",
  "version": "1.0.0",
  "main": "App.js",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:watch": "eslint . --watch",
    "start": "pm2 delete ecosystem.config.js && node preStart.js",
    "start:pm2": "pm2 start ecosystem.config.js",
    "stop:pm2": "pm2 delete ecosystem.config.js",
    "restart:pm2": "pm2 restart ecosystem.config.js",
    "logs:pm2": "pm2 logs",
    "status:pm2": "pm2 status",
    "dev": "pm2 delete ecosystem.config.js && nodemon App.js",
    "start:cluster": "pm2 start ecosystem.config.js --watch",
    "nginx:start": "sudo nginx",
    "nginx:stop": "sudo nginx -s stop",
    "nginx:stop-force": "sudo pkill -9 nginx",
    "nginx:restart": "sudo nginx -s stop && sudo nginx",
    "nginx:reload": "sudo nginx -s reload",
    "nginx:test": "sudo nginx -t",
    "nginx:status": "ps aux | grep nginx",
    "nginx:logs": "tail -f /usr/local/var/log/nginx/error.log",
    "nginx:logs-access": "tail -f /usr/local/var/log/nginx/access.log",
    "nginx:config": "code /usr/local/etc/nginx/nginx.conf",
    "nginx:fix-port80": "sudo lsof -i :80 | awk 'NR>1 {print $2}' | xargs sudo kill -9"
  },
  "author": "Marew Masresha Abate",
  "target": "node",
  "license": "ISC",
  "description": "A track management system like Uber Freight",
  "dependencies": {
    "@socket.io/redis-adapter": "^8.3.0",
    "axios": "^1.13.2",
    "basic-ftp": "^5.0.5",
    "bcryptjs": "^3.0.2",
    "body-parser": "^1.20.4",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.22.1",
    "express-async-errors": "^3.1.1",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^8.1.0",
    "firebase-admin": "^13.5.0",
    "helmet": "^8.1.0",
    "http": "^0.0.1-security",
    "ioredis": "^5.6.1",
    "joi": "^18.0.2",
    "jsonwebtoken": "^9.0.2",
    "kill-port": "^2.0.1",
    "morgan": "^1.10.1",
    "multer": "^1.4.5-lts.2",
    "mysql2": "^3.10.1",
    "os": "^0.1.2",
    "path": "^0.12.7",
    "pm2": "^6.0.8",
    "redis": "^5.0.0",
    "socket.io": "^4.8.1",
    "stream": "^0.0.3",
    "uuid": "^10.0.0",
    "winston": "^3.19.0",
    "xss-clean": "^0.1.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@jest/globals": "^30.2.0",
    "eslint": "^9.14.0",
    "globals": "^17.0.0",
    "jest": "^30.2.0",
    "supertest": "^7.2.2"
  }
}
